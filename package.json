{
  "name": "react-security-audit-plugin",
  "version": "1.0.0",
  "description": "A CLI + server plugin to audit React (v16 & v18) projects for performance and security issues, generate a structured report, and serve a UI to review and download the report.",
  "main": "src/index.js",
  "bin": {
    "rsaudit": "./src/cli.js"
  },
  "scripts": {
    "start-server": "node src/server/server.js",
    "build-ui": "(cd ui-src && npm run build) ",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@babel/parser": "^7.21.0",
    "@babel/traverse": "^7.21.0",
    "commander": "^9.0.0",
    "express": "^4.18.2",
    "globby": "^13.1.2",
    "puppeteer": "^19.0.0",
    "docx": "^7.0.0",
    "jszip": "^3.10.1",
    "prettier": "^2.8.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.29.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "MIT"
}
